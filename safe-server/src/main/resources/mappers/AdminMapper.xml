<?xml version="1.0" encoding="UTF-8" ?>
<!DOCTYPE mapper
        PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "https://mybatis.org/dtd/mybatis-3-mapper.dtd">
<!-- namespace等于mapper接口类的全限定名,这样实现对应 -->
<mapper namespace="team.ybj.mappers.AdminMapper">

    <select id="findAdminByUsername" resultType="team.ybj.pojo.YbjAdmin">
        select * from ybj_admin where username=#{username}
    </select>

</mapper>